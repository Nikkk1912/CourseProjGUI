<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.courseprojgui.fxControllers.CommentCreationWindowController">
   <children>
      <Slider fx:id="ratingSlider" layoutX="136.0" layoutY="115.0" prefHeight="26.0" prefWidth="329.0" />
      <TextArea fx:id="reviewField" layoutX="136.0" layoutY="157.0" prefHeight="159.0" prefWidth="329.0" promptText="Leave a review">
         <font>
            <Font size="14.0" />
         </font>
      </TextArea>
      <TextField fx:id="commentTitleField" layoutX="136.0" layoutY="79.0" prefHeight="25.0" prefWidth="329.0" />
      <Button fx:id="submitButton" onAction="#saveComment" layoutX="260.0" layoutY="341.0" mnemonicParsing="false" text="Submit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
